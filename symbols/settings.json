{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[javascript]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[python]": {},
  "[typescript]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[vue]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "commentTranslate.multiLineMerge": true,
  "commentTranslate.targetLanguage": "zh-CN",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": null,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.supportUntrustedWorkspaces": {
    "byted-ide.dev-environment-manager": {
      "supported": true
    }
  },
  "files.exclude": {
    "node_modules": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "off",
  "go.formatFlags": [
    "-s"
  ],
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.SSH.defaultExtensions": [
    "byted-ide.gallery"
  ],
  "remote.SSH.useLocalServer": false,
  "rest-client.defaultHeaders": {
    "User-Agent": "vscode-restclient"
  },
  "rest-client.environmentVariables": {},
  "rest-client.previewOption": "exchange",
  "security.workspace.trust.untrustedFiles": "open",
  // "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "FiraMono Nerd Font",
  "terminal.integrated.scrollback": 3000,
  "terminal.integrated.tabs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "update.mode": "none",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "yaml.schemas": {
    "kubernetes": [
      "*deploy.yml"
    ]
  },
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.workspaceFolder": "/home/urie/workplace/leetcode",
  "leetcode.defaultLanguage": "typescript",
  "rest-client.followredirect": false,
  "git.openRepositoryInParentFolders": "never",
  "remote.SSH.remotePlatform": {
    "home.lubui.com": "linux",
    "devbox": "linux",
    "cloudide-n4kvp3l8w3x8y7": "linux",
    "cloudide-yq9x902p7wwqpr": "linux",
    "cloudide-jw62n7xdynyl25": "linux",
    "CloudIDE_231252": "linux",
    "cloudide-67x80dvnj47p9l": "linux",
    "CloudIDE_182980": "linux",
    "cloudide-q4w4vq94pk8vr5": "linux",
    "CloudIDE_182780": "linux",
    "cloudide-wq61rox65v6v83": "linux",
    "mac.home.lubui.com": "macOS",
    "pc.home.lubui.com": "linux",
    "cloudide_231252": "linux",
    "cloudide-ws086518efa8354bf4aaad7019783be954": "linux",
    "cloudide_182780": "linux"
  },
  "byted-ide.settings-sync.includedExtensions": [],
  "workbench.startupEditor": "none",
  // "editor.tabCompletion": "onlySnippets",
  "[yaml]": {
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "off",
      "strings": "on"
    },
    "editor.defaultFormatter": "urie.yrtool"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "yrtool.formatters": [
    {
      "languages": [
        "sql",
        "javascript",
        "typescript",
        "yaml",
        "toml",
        "shellscript",
        "fish",
        "go",
        "markdown",
        "css",
        "scss",
        "vue",
        "json",
        "jsonc",
        "html",
        "less",
        "python",
        "typescriptreact",
      ],
      "command": "fmt_file",
      "args": [
        "-n",
        "$FILENAME",
        "-l",
        "$LANGUAGE",
      ]
    },
    {
      "languages": [
        "plaintext"
      ],
      "command": "fmt_file",
      "args": [
        "-n",
        "$FILENAME",
      ]
    }
  ],
  "python.terminal.activateEnvironment": false,
  "editor.indentSize": "tabSize",
  "[fish]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "[toml]": {
    "editor.defaultFormatter": "urie.yrtool"
  },
  "window.confirmBeforeClose": "always",
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.defaultFormatter": "urie.yrtool",
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.tabCompletion": "on",
  "resmon.show.cpuusage": false,
  "resmon.show.cpufreq": false,
  "resmon.show.mem": false,
  "resmon.show.battery": false,
  "resmon.show.disk": false,
  "resmon.show.cputemp": false,
}