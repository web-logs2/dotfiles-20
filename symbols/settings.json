{
  "C_Cpp.formatting": "vcFormat",
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "C_Cpp.intelliSenseEngineFallback": "Disabled",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "Vehmloewff.custom-format"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {},
  "[typescript]": {
    "editor.defaultFormatter": "rome.rome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "arduino.ignoreBoards": [],
  "arduino.path": "/usr/local/bin/arduino-cli",
  "arduino.useArduinoCli": true,
  "cSpell.userWords": [
    "consts",
    "ctxservice",
    "Kubernetes",
    "yrtool"
  ],
  "commentTranslate.multiLineMerge": true,
  "commentTranslate.targetLanguage": "zh-CN",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": null,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.supportUntrustedWorkspaces": {
    "byted-ide.dev-environment-manager": {
      "supported": true
    }
  },
  "files.exclude": {
    "node_modules": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "go": true,
    "rust": false,
    "yaml": false
  },
  "gitlab.instanceUrl": "https://code.byted.org",
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "off",
  "go.formatFlags": [
    "-s"
  ],
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.latexindent.path": "/usr/local/texlive/2021/bin/universal-darwin/latexindent",
  "latex-workshop.view.pdf.viewer": "tab",
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.SSH.defaultExtensions": [
    "byted-ide.gallery"
  ],
  "remote.SSH.useLocalServer": false,
  "resmon.show.battery": false,
  "resmon.show.cpufreq": false,
  "rest-client.defaultHeaders": {
    "User-Agent": "vscode-restclient"
  },
  "rest-client.environmentVariables": {},
  "rest-client.previewOption": "exchange",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "FiraMono Nerd Font",
  "terminal.integrated.scrollback": 3000,
  "terminal.integrated.tabs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "update.mode": "none",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "yaml.schemas": {
    "kubernetes": [
      "*deploy.yml"
    ]
  },
  "leetcode.endpoint": "leetcode-cn",
  "leetcode.workspaceFolder": "/home/urie/workplace/leetcode",
  "leetcode.defaultLanguage": "typescript",
  "rest-client.followredirect": false,
  "git.openRepositoryInParentFolders": "never",
  "remote.SSH.remotePlatform": {
    "home.lubui.com": "linux",
    "devbox": "linux",
    "cloudide-n4kvp3l8w3x8y7": "linux",
    "cloudide-yq9x902p7wwqpr": "linux",
    "cloudide-jw62n7xdynyl25": "linux",
    "CloudIDE_231252": "linux",
    "cloudide-67x80dvnj47p9l": "linux",
    "CloudIDE_182980": "linux",
    "cloudide-q4w4vq94pk8vr5": "linux",
    "CloudIDE_182780": "linux",
    "cloudide-wq61rox65v6v83": "linux",
    "mac.home.lubui.com": "macOS",
    "pc.home.lubui.com": "linux"
  },
  "byted-ide.settings-sync.includedExtensions": [],
  "workbench.startupEditor": "none",
  // "editor.tabCompletion": "onlySnippets",
  "[yaml]": {
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "off",
      "strings": "on"
    },
    "editor.defaultFormatter": "Vehmloewff.custom-format"
  },
  "editor.acceptSuggestionOnEnter": "off",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "custom-format.formatters": [
    {
      "language": "shellscript",
      "command": "fmt_file -l bash"
    },
    {
      "language": "fish",
      "command": "fmt_file -l fish"
    },
    {
      "language": "sql",
      "command": "fmt_file -l sql"
    },
    {
      "language": "dockerfile",
      "command": "fmt_file -l dockerfile"
    },
    {
      "language": "javascript",
      "command": "fmt_file -l js"
    },
    {
      "language": "typescript",
      "command": "fmt_file -l ts"
    },
    {
      "language": "json",
      "command": "fmt_file -l json"
    },
    {
      "language": "yaml",
      "command": "fmt_file -l yaml"
    },
    {
      "language": "toml",
      "command": "fmt_file -l toml"
    },
  ],
  "[fish]": {
    "editor.defaultFormatter": "Vehmloewff.custom-format"
  },
  "python.terminal.activateEnvironment": false,
  "[toml]": {
    "editor.defaultFormatter": "Vehmloewff.custom-format"
  },
}