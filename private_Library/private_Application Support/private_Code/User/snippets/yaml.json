{
  "docker-compose": {
    "prefix": "docker",
    "body": [
      "services:",
      "  $1:",
      "    container_name: $1",
      "    image: nyanmisaka/jellyfin",
      "    environment:",
      "      PUID: 1000",
      "      PGID: 1000",
      "      TZ: Asia/Shanghai",
      "      VIRTUAL_HOST: $1.home.lubui.com",
      "      VIRTUAL_PORT: ",
      "    devices:",
      "      - /dev/dri:/dev/dri",
      "    volumes:",
      "      - ./config:/config",
      "    network_mode: host",
      "    restart: unless-stopped",
      "    labels:",
      "      - com.centurylinklabs.watchtower.enable=true"
    ]
  }
}
